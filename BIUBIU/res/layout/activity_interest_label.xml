<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   	android:orientation="vertical"
    >
    <RelativeLayout
        android:id="@+id/top_selector_personality_tag_rl"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@color/page_text3" >

        <RelativeLayout
            android:id="@+id/back_interest_lable_mine_rl"
            android:layout_width="50dp"
            android:layout_height="44dp"
            android:layout_centerVertical="true"
            android:background="@drawable/kongjianruoqu_bg"
            android:gravity="center" >

            <ImageView
                android:id="@+id/back_changexingzuo_mine"
                android:layout_width="10dp"
                android:layout_height="17dp"
                android:src="@drawable/main_name_btn_back" />
        </RelativeLayout>

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/page_interest"
            android:textColor="@color/white"
            android:textSize="18sp" />

       
	<RelativeLayout 
	    android:id="@+id/complete_interest_lable_rl"
	    android:layout_width="65dp"
	    android:layout_height="44dp"
	     android:layout_alignParentRight="true"
	     android:gravity="center"
	    android:background="@drawable/kongjianruoqu_bg">
        <TextView
            android:id="@+id/complete_interest_lable_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
          	android:textSize="13sp"
           	android:textColor="@color/white"
            android:text="@string/mine_complete" />
        </RelativeLayout>

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#cccccc" />
    <FrameLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <ListView 
        android:id="@+id/id_listView_intetest_lable"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:cacheColorHint="#00000000"
        android:fadingEdge="none"
        android:listSelector="@android:color/transparent"
        android:scrollingCache="false"
        android:divider="@null" 
        />
    
    <include layout="@layout/loading_layout"/>
    <include layout="@layout/load_error_layout"/>
</FrameLayout>

</LinearLayout>